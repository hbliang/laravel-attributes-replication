{
    "name": "hbliang/laravel-sync-attributes-between-models",
    "description": "Laravel Sync with attributes with models",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hbliang",
            "email": "ishaobinliang@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Hbliang\\AttributesReplication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hbliang\\AttributesReplication\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hbliang\\AttributesReplication\\ReplicationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
